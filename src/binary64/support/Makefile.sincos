# -*- makefile -*-
# for sincos-like functions (one input, two outputs)

include ../../generic/support/Makefile.checkstd

CFLAGS += -W -Wall $(ROUNDING_MATH)

TYPE_UNDER_TEST := double

all:: check_worst

check_worst: check_worst_sincos.o $(CORE_MATH_OBJS) $(FUNCTION_UNDER_TEST)_mpfr.o
	$(CC) $(LDFLAGS) $(OPENMP) -o $@ $^ -lmpfr -lm

%.o: %.c
	$(CC) $(CFLAGS) -I ../support -c -o $@ $<

check_worst_sincos.o: ../support/check_worst_sincos.c
	$(CC) $(OPENMP) $(CFLAGS) $(CORE_MATH_DEFINES) -I . -c -o $@ $<

clean::
	rm -f *.o check_worst

include ../../generic/support/Makefile.sincos
